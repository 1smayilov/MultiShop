@model GetCargoCustomerByIdDto

@{
    ViewData["Title"] = "UserAddressInfo";
    Layout = "~/Areas/Admin/Views/AdminLayout/Index.cshtml";
    int count = 0;
}

<section class="content container-fluid">
    <div class="row">

        <div class="col-md-12">
            <div class="chart-box">
                <div class="bs-example" data-example-id="bordered-table">

                    <h3>İstifadəçi Adres və Əlaqə Malumatı</h3>
                    <div class="alert alert-info">İstifadəçi Id:@Model.cargoCustomerId</div>
                    <div class="alert alert-warning">İstifadəçi Adı Soyadı: @Model.name @Model.surname</div>
                    <div class="alert alert-info">Email Adresi: @Model.email</div>
                    <div class="alert alert-warning">Telefon: @Model.phone</div>
                    <div class="alert alert-info">Rayon/Şəhər: @Model.district @Model.city</div>
                    <div class="alert alert-warning">Adres: @Model.address</div>
                </div>
            </div>
        </div>
    </div>



    </div>
</section>

